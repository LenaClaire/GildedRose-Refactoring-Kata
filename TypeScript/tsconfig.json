{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es5",
    "strict": true,
    "noImplicitAny": false,
    "sourceMap": true,
    "baseUrl": "./",
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strictNullChecks": true,
    "noUncheckedIndexedAccess": true,
    "paths": {
      "@/*": [
        "app/*"
      ]
    },
    "types": [
      "vitest/globals"
    ]
  },
  "exclude": [
    "node_modules"
  ]
}
